@{
    ViewBag.PageTitle = "لیست آلارم آفات";
}

<div class="row headerbox">
    <div class="col-sm-12">
        <div class="project-sort pull-left">
            <div class="project-sort-item">
                <div class="form-inline">
                    <div class="form-group">
                        <label>انتخاب پروژه :</label>
                        <select id="PestAlarmReport_Project" asp-items="@ViewBag.ProjectList" class="form-control input-sm" style="width: 180px;margin: 3px 0 10px 10px;">
                            <option selected>تمام موارد</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label>انتخاب ایستگاه :</label>
                        <select id="PestAlarmReport_Station" asp-items="@ViewBag.StationList" class="form-control input-sm" style="width: 180px;margin: 3px 0 10px 10px;">
                            <option selected>تمام موارد</option>
                        </select>
                    </div>
                                 </div>
            </div>
        </div>
    </div><!-- end col-->
</div>
<div class="row">
    <div class="col-sm-12">
        <div class="card-box table-responsive">
            <h4 class="header-title m-t-0 m-b-30" id="PestAlarmReport_ListTitle">لیست آلارم آفات</h4>

            <div class="row" id="PestAlarmReport_List">

            </div>
        </div>
    </div><!-- end col -->
</div>


