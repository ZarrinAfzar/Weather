@using Weather.Data.Base
@model Weather.Data.ViewModel.BaseInfoVm

@{
    ViewBag.PageTitle = "اطلاعات پایه";
}

<div class="row m-b-20">
    @{
        if (User.IsInRole("AdminiStratore") || User.IsInRole("BaseInfoState"))
        {
            <div class="col-lg-6">
                <div class="card-box">
                    <h4 class="header-title m-t-0 m-b-5 basePage_Header">استان ها</h4>
                    <div class="row">
                        <div class="col-md-12" style="border-bottom: 1px dotted #188ae2; padding-bottom: 5px; margin-bottom: 5px;height: 120px;">
                            <label class="col-md-2 control-label" style="padding-top: 5px;text-align: left">استان :</label>
                            <div class="col-md-8">
                                <input type="hidden" id="State_Id" value="0">
                                <input type="text" id="State_Name" class="form-control input-sm" placeholder="نام استان را وارد کنید">
                            </div>
                            <button onclick="State_Save()" class="btn btn-icon waves-effect waves-light btn-primary m-b-5" style="padding: 5px 15px 2px 15px;"> <i class="fa fa-save"></i> </button>
                        </div>
                        <div class="col-md-12">
                            <div style="height: 200px;overflow-y: auto;">
                                <ul class="list-group" id="State_List">
                                    @{
                                        foreach (var item in Model.States)
                                        {
                                            <li class="list-group-item" id="StateItem_@item.Id">
                                                <span onclick="State_Delete(@item.Id)" class="badge badge-primary listStyleButton"><i class="fa fa-trash-o"></i></span>
                                                <span onclick="State_Get(@item.Id)" class="badge badge-primary listStyleButton"><i class="fa fa-pencil"></i></span>
                                                <text id="StateItemName_@item.Id">@item.Name</text>
                                            </li>
                                        }
                                    }
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div> <!-- BaseInfoState -->
        }
        if (User.IsInRole("AdminiStratore") || User.IsInRole("BaseInfoCities"))
        {
            <div class="col-lg-6">
                <div class="card-box">
                    <h4 class="header-title m-t-0 m-b-5 basePage_Header">شهرستان ها</h4>

                    <div class="row">
                        <div class="col-md-12" style="border-bottom: 1px dotted #188ae2; padding-bottom: 5px; margin-bottom: 5px;height: 120px;">
                            <div class="col-md-12">
                                <label class="col-md-2 control-label" style="padding-top: 5px;text-align: left">استان :</label>
                                <div class="col-md-8">
                                    <select class="form-control input-sm" id="city_StateId" asp-items="@ViewBag.StateList">
                                        <option>پروژه...</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-12 m-t-5">
                                <label class="col-md-2 control-label" style="padding-top: 5px;text-align: left">شهرستان :</label>
                                <div class="col-md-8">
                                    <input type="hidden" id="City_Id" value="0">
                                    <input type="text" id="city_Name" class="form-control input-sm" placeholder="نام استان را وارد کنید">
                                </div>
                                <button onclick="City_Save()" class="btn btn-icon waves-effect waves-light btn-primary m-b-5" style="padding: 5px 15px 2px 15px;"> <i class="fa fa-save"></i> </button>
                            </div>

                        </div>
                        <div class="col-md-12">
                            <div style="height: 200px;overflow-y: auto;">
                                <ul class="list-group" id="City_List">
                                    @{
                                        foreach (var item in Model.Cities)
                                        {
                                            <li class="list-group-item" id="CityItem_@item.Id">
                                                <span onclick="City_Delete(@item.Id)" class="badge badge-primary listStyleButton"><i class="fa fa-trash-o"></i></span>
                                                <span onclick="City_Get(@item.Id)" class="badge badge-primary listStyleButton"><i class="fa fa-pencil"></i></span>
                                                <p style="margin: 0;" id="CityItemName_@item.Id"><b> @item.Name</b> <text style="font-size: 11px;color: #188ae2;">@item.State.Name</text></p>
                                            </li>
                                        }
                                    }
                                </ul>
                            </div>
                        </div>
                    </div>

                </div>
            </div><!-- BaseInfoCities -->
        }
    }
</div>
<div class="row m-b-20">
