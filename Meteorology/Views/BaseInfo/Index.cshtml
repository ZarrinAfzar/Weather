<div class="row m-b-20">
    @{
        if (User.IsInRole("AdminiStratore") || User.IsInRole("SensorType"))
        {
            <div class="col-lg-12">
                <div class="card-box">
                    <h4 class="header-title m-t-0 m-b-5 basePage_Header">نام استاندارد سنسورها</h4>
                    <div class="row">
                        <div class="col-md-12" style="border-bottom: 1px dotted #188ae2; padding-bottom: 5px; margin-bottom: 5px;height: 110px;">
                            <div class="row">
                                <div class="col-md-6">
                                    <input type="hidden" id="SensorTypes_Id" value="0">
                                    <div class="col-md-12 m-t-5">
                                        <label class="col-md-2 control-label text-right">نام فارسی :</label>
                                        <div class="col-md-8">
                                            <input type="text" id="SensorTypes_FaName" class="form-control input-sm" placeholder="نام فارسی را وارد کنید">
                                        </div>
                                    </div>
                                    @*<div class="col-md-12 m-t-5">
                                        <label class="col-md-2 control-label text-right">بیشترین مقدار :</label>
                                        <div class="col-md-8">
                                            <input type="text" id="SensorTypes_Max" class="form-control input-sm" placeholder="بیشترین مقدار را وارد کنید">
                                        </div>
                                    </div>*@
                                    <div class="col-md-12 m-t-5">
                                        <label class="col-md-2 control-label text-right">نوع سنسور :</label>
                                        <div class="col-md-8">
                                            <div class="radio  radio-inline radio-success" style="margin-right: -20px;">
                                                <input type="radio" name="SensorTypes_SensorType_State" id="Regular" value="1">
                                                <label for="Regular"> سنسور حقیقی </label>
                                            </div>
                                            <div class="radio  radio-inline radio-success m-r-10">
                                                <input type="radio" name="SensorTypes_SensorType_State" id="Virtual" value="2">
                                                <label for="Virtual"> سنسور مجازی </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="col-md-12 m-t-5">
                                        <label class="col-md-2 control-label text-right">نام لاتین :</label>
                                        <div class="col-md-8">
                                            <input type="text" id="SensorTypes_EnName" class="form-control input-sm m-b-5" placeholder="نام لاتین را وارد کنید">
                                        </div>
                                        <button onclick="SensorTypes_Save()" class="btn btn-icon waves-effect waves-light btn-primary m-b-5" style="padding: 5px 15px 2px 15px;"> <i class="fa fa-save"></i> </button>
                                    </div>
                                    @*<div class="col-md-12 m-t-5">
                                        <label class="col-md-2 control-label text-right">کمترین مقدار :</label>
                                        <div class="col-md-8">
                                            <input type="text" id="SensorTypes_Min" class="form-control input-sm" placeholder="کمترین مقدار را وارد کنید">
                                        </div>
                                    </div>*@
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div style="height: 200px;overflow-y: auto;">
                                <table class="table table-striped m-0">
                                    <thead>
                                        <tr>
                                            <th>نام فارسی</th>
                                            <th>نام لاتین</th>
                                            @*<th>کمترین مقدار</th>
                                            <th>بیشترین مقدار</th>*@
                                            <th>نوع سنسور</th>
                                            <th>#</th>
                                        </tr>
                                    </thead>
                                    <tbody id="SensorTypes_List">
                                        @{
                                            foreach (var item in Model.SensorTypes)
                                            {
                                                <tr id="SensorTypes_ListItem_@item.Id">
                                                    <td>@item.FaName</td>
                                                    <td>@item.EnName</td>
                                                    @*<td>@item.Min</td>
                                                    <td>@item.Max</td>*@
                                                    <td>@item.SensorType_State.GetDisplayName()</td>
                                                    <td>
                                                        <span onclick="SensorTypes_Delete(@item.Id)" class="badge badge-primary listStyleButton"><i class="fa fa-trash-o"></i></span>
                                                        <span onclick="SensorTypes_Get(@item.Id)" class="badge badge-primary listStyleButton"><i class="fa fa-pencil"></i></span>
                                                    </td>
                                                </tr>
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div><!-- end col -->
        }
    }
</div>
