                            </div>
                        </div>
                        <div class="col-md-12">
                            <div style="height: 200px;overflow-y: auto;">
                                <ul class="list-group" id="StationType_List">
                                    @{
                                        foreach (var item in Model.StationTypes)
                                        {
                                            <li class="list-group-item" id="StationTypeItem_@item.Id">
                                                <span onclick="StationType_Delete(@item.Id)" class="badge badge-primary listStyleButton"><i class="fa fa-trash-o"></i></span>
                                                <span onclick="StationType_Get(@item.Id)" class="badge badge-primary listStyleButton"><i class="fa fa-pencil"></i></span>
                                                @*<img id="StationTypeItemIcon_@item.Id" src="@item.IconPath" style="width:24px;margin-left: 10px;border: 1px solid #bbb;border-radius: 100px;" />*@
                                                <text id="StationTypeItemName_@item.Id">@item.Name</text>
                                            </li>
                                        }
                                    }
                                </ul>
                            </div>
                        </div>
                    </div>

                </div>
            </div><!-- end col -->
        }
    }
</div>
