@model Project
<div class="modal-body">
    <h5><b style="float: right; margin-bottom: 5px;">کد :</b></h5>
    <h4 class="m-t-0 m-b-5"><a href="" class="text-inverse">@Model.Code</a></h4>

    <div class="m-t-20">
        <h5><b style="float: right; margin-bottom: 5px;">عنوان :</b></h5>
        <h4 class="m-t-0 m-b-5"><a href="" class="text-inverse">@Model.Name</a></h4>
    </div>
    <div class="m-t-20">
        <h5><b style="float: right; margin-bottom: 5px;">توضیحات :</b></h5>
        <p class="text-muted font-13">
            @Model.Detail
        </p>
    </div>
</div>
